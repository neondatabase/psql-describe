{
  "name": "psql-describe",
  "version": "1.0.0",
  "description": "psql's `\\d` (describe) family of commands ported to JavaScript.",
  "main": "index.js",
  "scripts": {
    "test": "test/test.mjs /usr/local/pgsql/bin/psql postgres://localhost:5435/main < test/tests.txt",
    "build": "esbuild src/describe.mjs --outfile=dist/describe.min.js --platform=neutral --target=es2020 --minify --line-limit=120",
    "bundleDemo": "esbuild demo-src/demo.js --bundle --outfile=demo/demo.js --platform=neutral --target=es2020"
  },
  "author": "George MacKerron",
  "license": "postgresql",
  "devDependencies": {
    "@neondatabase/serverless": "^0.6.0",
    "esbuild": "^0.19.5",
    "pg": "^8.11.3"
  }
}
